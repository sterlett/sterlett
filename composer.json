{
    "name": "sterlett/sterlett",
    "type": "project",
    "description": "",
    "homepage": "https://github.com/sterlett/sterlett",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Pavel Petrov",
            "email": "itnelo@gmail.com",
            "homepage": "https://ppetrov.it",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sterlett\\": [
                "src/back/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sterlett\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-pcntl": ">=7.4",
        "psr/container": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.1",
        "react/event-loop": "^1.1",
        "react/http": "^0.8.6",
        "react/stream": "^1.1",
        "symfony/config": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/dotenv": "^5.1",
        "symfony/event-dispatcher": "^5.1",
        "symfony/yaml": "^5.1",
        "wyrihaximus/react-psr-3-stdio": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "symfony/event-dispatcher-contracts": "^2.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}

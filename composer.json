{
    "name": "sterlett/sterlett",
    "type": "project",
    "description": "The microservice for retrieving and processing public information about computer hardware prices",
    "keywords": ["hardware", "microservice"],
    "homepage": "https://github.com/sterlett/sterlett",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Pavel Petrov",
            "email": "itnelo@gmail.com",
            "homepage": "https://ppetrov.it",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sterlett\\": [
                "src/back/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sterlett\\Tests\\": [
                "tests/unit/"
            ]
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-ds": "^1.3",
        "ext-pcntl": ">=7.4",
        "clue/block-react": "^1.4",
        "clue/http-proxy-react": "^1.6",
        "itnelo/reactphp-webdriver": "^0.2",
        "psr/container": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.1",
        "react/event-loop": "^1.1",
        "react/http": "^1.2",
        "react/promise": "^2.8",
        "react/stream": "^1.1",
        "ringcentral/psr7": "^1.3",
        "symfony/config": "^5.2",
        "symfony/console": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/event-dispatcher": "^5.2",
        "symfony/options-resolver": "^5.2",
        "symfony/property-access": "^5.2",
        "symfony/yaml": "^5.2",
        "wyrihaximus/react-psr-3-stdio": "^2.1"
    },
    "require-dev": {
        "php-ds/php-ds": "^1.3",
        "phpunit/phpunit": "^9.5",
        "symfony/event-dispatcher-contracts": "^2.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
